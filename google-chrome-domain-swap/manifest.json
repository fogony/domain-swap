{
	"name": "Domain Swap UniMelb",
	"version": "3.0.0",
	"manifest_version": 2,
	"description": "Switch domains without switching paths. UniMelb version with default URL settings.",
	"homepage_url": "https://github.com/fogony/domain-swap",
	"icons": {
		"128": "icon-128x128.png",
		"64": "icon-64x64.png",
		"32": "icon-32x32.png",
		"19": "icon-19x19.png"
	},
	"background": { "scripts": [ "domain-sets.js", "storage.js", "domain-switcher.js", "background.js", "default-storage.js" ] },
	"page_action": {
		"default_icon": "icon-19x19.png",
		"default_title": "Switch Domain"
	},
	"options_page": "options.html",
	"permissions": [ "tabs", "storage" ]
}
