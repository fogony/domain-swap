{
	"name": "Domain Swap",
	"version": "2.0.1",
	"manifest_version": 2,
	"description": "Switch domains without switching paths.",
	"homepage_url": "https://github.com/jmhobbs/google-chome-domain-switcher",
	"icons": {
		"128": "icon-128x128.png",
		"64": "icon-64x64.png",
		"32": "icon-32x32.png",
		"19": "icon-19x19.png"
	},
	"background": { "scripts": [ "domain-sets.js", "storage.js", "domain-switcher.js", "background.js" ] },
	"page_action": {
		"default_icon": "icon-19x19.png",
		"default_title": "Switch Domain"
	},
	"options_page": "options.html",
	"permissions": [ "tabs" ]
}
