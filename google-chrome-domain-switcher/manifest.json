{
  "name": "Domain Switcher",
  "version": "1.0",
	"manifest_version": 2,
  "description": "Switch domains without switching paths.",
  "icons": {
		"128": "icon-128x128.png",
		"64": "icon-64x64.png",
		"32": "icon-32x32.png",
		"19": "icon-19x19.png"
	},
  "browser_action": {
    "default_icon": "icon.png",
		"default_popup": "popup.html"
  },
	"options_page": "options.html",
  "permissions": [ "tabs" ]
}
