{
  "name": "Domain Switcher",
  "version": "1.0",
	"manifest_version": 2,
  "description": "Switch domains without switching paths.",
  "icons": {"128": "icon.png"},
  "browser_action": {
    "default_icon": "icon.png",
		"default_popup": "popup.html"
  },
	"options_page": "options.html",
  "permissions": [ "tabs" ]
}
